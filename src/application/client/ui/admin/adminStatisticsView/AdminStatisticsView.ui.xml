<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:HTMLPanel styleName="center-content">

        <g:Label text="Medlemsstatistik" styleName="headline" />
        <g:HTMLPanel>
            <g:Label text="Alle medlemmer" />
            <g:HTMLPanel styleName="stats-wrapper">
                <g:HTMLPanel styleName="age">
                    <g:Label text="70+" />
                    <g:Label text="60-69" />
                    <g:Label text="50-59" />
                    <g:Label text="40-49" />
                    <g:Label text="30-39" />
                    <g:Label text="20-29" />
                    <g:Label text="15-19" />
                </g:HTMLPanel>
                <g:HTMLPanel styleName="stats-content">
                    <g:HTMLPanel styleName="stats">
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_7"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_6"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_5"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_4"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_3"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_2"></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="row">
                            <g:HTMLPanel styleName="item" ui:field="item1_1"></g:HTMLPanel>
                        </g:HTMLPanel>
                    </g:HTMLPanel>
                    <g:HTMLPanel styleName="percentage">
                        <g:Label text="0%" />
                        <g:Label text="10%" />
                        <g:Label text="20%" />
                        <g:Label text="30%" />
                        <g:Label text="40%" />
                        <g:Label text="50%" />
                        <g:Label text="60%" />
                        <g:Label text="70%" />
                        <g:Label text="80%" />
                        <g:Label text="90%" />
                        <g:Label text="100%" />
                    </g:HTMLPanel>
                </g:HTMLPanel>
            </g:HTMLPanel>
        </g:HTMLPanel>

    </g:HTMLPanel>
</ui:UiBinder>